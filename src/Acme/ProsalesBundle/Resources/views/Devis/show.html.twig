{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Devis</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Dossier</th>
                <td>{{ entity.dossier }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ entity.reference }}</td>
            </tr>
            <tr>
                <th>Totalht</th>
                <td>{{ entity.totalht }}</td>
            </tr>
            <tr>
                <th>Tauxtva</th>
                <td>{{ entity.tauxtva }}</td>
            </tr>
            <tr>
                <th>Totaltva</th>
                <td>{{ entity.totaltva }}</td>
            </tr>
            <tr>
                <th>Totalttc</th>
                <td>{{ entity.totalttc }}</td>
            </tr>
            <tr>
                <th>Fraisport</th>
                <td>{{ entity.fraisport }}</td>
            </tr>
            <tr>
                <th>Observation</th>
                <td>{{ entity.observation }}</td>
            </tr>
            <tr>
                <th>Raisonclassement</th>
                <td>{{ entity.raisonclassement }}</td>
            </tr>
            <tr>
                <th>Classement</th>
                <td>{{ entity.classement }}</td>
            </tr>
            <tr>
                <th>Envoimail</th>
